<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<HBox stylesheets="@../../styles/test_row.css" xmlns="http://javafx.com/javafx/17.0.12"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="av.entrance.client.controller.user.items.TestRowController"
      spacing="20" alignment="CENTER_LEFT">

    <padding>
        <Insets top="5" right="20" bottom="5" left="20"/>
    </padding>
    <Label fx:id="sNo" text="1." prefWidth="40"/>
    <Label fx:id="testName" text="Test Name" prefWidth="240"/>
    <Label fx:id="duration" prefWidth="140" text="1 Hour" textAlignment="CENTER"/>
    <Button fx:id="deleteTest" text="Delete Test" onAction="#delete"/>

</HBox>
